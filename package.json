{
  "name": "skiz-parser",
  "description": "Parse a .skiz file exported from Ski Tracks",
  "version": "0.0.0-development",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "2m"
  },
  "bugs": {
    "url": "https://github.com/kirkeaton/skiz-parser/issues"
  },
  "dependencies": {
    "csv-parser": "3.0.0",
    "fast-xml-parser": "4.2.5",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@types/node": "20.4.5",
    "ava": "5.3.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "semantic-release": "21.0.6",
    "tsd": "0.28.1"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": "./index.js",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "homepage": "https://github.com/kirkeaton/skiz-parser#readme",
  "keywords": [
    "file",
    "parse",
    "ski tracks",
    "skiz"
  ],
  "license": "BSD-3-Clause",
  "lint-staged": {
    "*.{js,json,md,ts}": "prettier --write"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirkeaton/skiz-parser.git"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "ava -v && tsd"
  },
  "type": "module",
  "types": "./index.d.ts"
}
