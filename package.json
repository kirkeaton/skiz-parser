{
  "name": "skiz-parser",
  "description": "Parse a .skiz file exported from Ski Tracks",
  "version": "0.0.0-development",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "2m"
  },
  "bugs": {
    "url": "https://github.com/kirkeaton/skiz-parser/issues"
  },
  "dependencies": {
    "csv-parser": "^3.0.0",
    "fast-xml-parser": "^3.19.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": ">=12"
  },
  "exports": "./index.js",
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/kirkeaton/skiz-parser#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "file",
    "parse",
    "ski tracks",
    "skiz"
  ],
  "license": "BSD-3-Clause",
  "lint-staged": {
    "*.{ts,tsx,json,md}": "prettier --write"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirkeaton/skiz-parser.git"
  },
  "scripts": {
